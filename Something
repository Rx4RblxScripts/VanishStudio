local WEBHOOK_URL = "https://discord.com/api/webhooks/1455121444760453151/CP4l97hX7pIhGRZFB977f6OL6Pez_XIzmBm5Hb1960DotxlwfZMx2iiUMZSI5ohwAXAX"
