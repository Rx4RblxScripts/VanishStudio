--// EXECUTION COUNTER
local ExecutionCount = 0

--// WEBHOOK FUNCTION
local startTime = tick()
local function sendWebhook(keyUsed)
	ExecutionCount += 1

	local gameName = "Unknown"
	pcall(function()
		gameName = MarketplaceService:GetProductInfo(game.PlaceId).Name
	end)

	local currentTime = tick()
	local sessionSeconds = currentTime - startTime
	local sessionMinutes = math.floor(sessionSeconds / 60)
	local sessionHours = math.floor(sessionMinutes / 60)
	local remainingMinutes = sessionMinutes % 60
	local sessionTimeStr = sessionHours .. "h " .. remainingMinutes .. "m"

	local payload = {
		username = "VanishStudios (Atoms) Logs",
		embeds = { {
			title = "✅ Script Executed",
			color = 1127128,
			thumbnail = { url = AVATAR_URL },
			fields = {
				{ name = "Player", value = LocalPlayer.Name, inline = true },
				{ name = "Account Age", value = AccountAge, inline = true },
				{ name = "Executor", value = Executor, inline = true },
				{ name = "HWID", value = HWID },
				{ name = "Game", value = gameName },
				{ name = "Key Used", value = keyUsed },
				{ name = "Headless", value = HasHeadless, inline = true },
				{ name = "Korblox", value = HasKorblox, inline = true },
				{ name = "Execution Count", value = tostring(ExecutionCount), inline = true },
				{ name = "Date Used", value = os.date("%Y-%m-%d %H:%M:%S"), inline = true },
				{ name = "Session Duration", value = sessionTimeStr, inline = true },
			},
			footer = { text = "Roblox Key System" },
			timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
		} }
	}

	pcall(function()
		httpreq({
			Url = WEBHOOK_URL,
			Method = "POST",
			Headers = { ["Content-Type"] = "application/json" },
			Body = HttpService:JSONEncode(payload)
		})
	end)
end

--// NOTIFICATION FUNCTION (unchanged)
local function sendNotification(title, text, isSuccess)
	local ScreenGui = Instance.new("ScreenGui")
	ScreenGui.Name = "XploitLog"
	ScreenGui.ResetOnSpawn = false
	ScreenGui.Parent = game.CoreGui

	local Frame = Instance.new("Frame", ScreenGui)
	Frame.Size = UDim2.new(0, 300, 0, 80)
	Frame.Position = UDim2.new(1, -320, 0, 50)
	Frame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
	Frame.BorderSizePixel = 0
	Instance.new("UICorner", Frame).CornerRadius = UDim.new(0, 12)

	local Gradient = Instance.new("UIGradient", Frame)
	if isSuccess then
		Gradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0, Color3.fromRGB(0,255,128)),
			ColorSequenceKeypoint.new(1, Color3.fromRGB(0,128,255))
		}
	else
		Gradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0, Color3.fromRGB(255,0,0)),
			ColorSequenceKeypoint.new(1, Color3.fromRGB(255,128,0))
		}
	end
	Gradient.Rotation = 0

	spawn(function()
		while Frame.Parent do
			for i = 0, 360, 2 do
				Gradient.Rotation = i
				task.wait(0.02)
			end
		end
	end)

	local TitleLabel = Instance.new("TextLabel", Frame)
	TitleLabel.Size = UDim2.new(1, -20, 0, 30)
	TitleLabel.Position = UDim2.new(0, 10, 0, 5)
	TitleLabel.BackgroundTransparency = 1
	TitleLabel.Text = title
	TitleLabel.Font = Enum.Font.GothamBold
	TitleLabel.TextSize = 19
	TitleLabel.TextColor3 = Color3.new(1,1,1)
	TitleLabel.TextXAlignment = Enum.TextXAlignment.Left

	local TextLabel = Instance.new("TextLabel", Frame)
	TextLabel.Size = UDim2.new(1, -20, 0, 40)
	TextLabel.Position = UDim2.new(0, 10, 0, 35)
	TextLabel.BackgroundTransparency = 1
	TextLabel.Text = text
	TextLabel.Font = Enum.Font.Gotham
	TextLabel.TextSize = 19
	TextLabel.TextColor3 = Color3.new(1,1,1)
	TextLabel.TextXAlignment = Enum.TextXAlignment.Left
	TextLabel.TextWrapped = true

	task.delay(4, function()
		if Frame then Frame:Destroy() end
	end)
end

