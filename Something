-- ╔═══════════════════════
-- ║     BLACKLIST
-- ╚═══════════════════════
local Blacklist = {
    ["pahila_destroyer"] = true,
    [""], = true,
    [""], = true,
    ["
    ["
    ["
}

-- Message shown when kicked
local BlacklistKickMessage = "You Are BlackListed From Using This Script Again!"

-- Check if current player is blacklisted (case-insensitive)
local function isBlacklisted()
    local nameLower = LocalPlayer.Name:lower()
    for blacklistedName in pairs(Blacklist) do
        if nameLower == blacklistedName:lower() then
            return true
        end
    end
    return false
end

-- Immediate blacklist check at start
if isBlacklisted() then
    task.wait(0.5)  -- small delay to let GUI appear briefly if you want
    LocalPlayer:Kick(BlacklistKickMessage)
    return  -- stop the entire script
end

