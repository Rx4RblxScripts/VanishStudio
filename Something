local AccountAge = LocalPlayer.AccountAge .. " days"
local AVATAR_URL = "https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds=" .. LocalPlayer.UserId .. "&size=420x420&format=Png&isCircular=false"

--// CHARACTER CHECKS
local function hasHeadless(character)
	local head = character:FindFirstChild("Head")
	if not head then return true end
	if head.Transparency == 1 and not head:FindFirstChildOfClass("Decal") then
		return true
	end
	return false
end

local function hasKorblox(character)
	for _, v in ipairs(character:GetDescendants()) do
		if v:IsA("SpecialMesh") or v:IsA("MeshPart") then
			if string.find(string.lower(v.Name), "korblox") then
				return true
			end
		end
	end
	return false
end

local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HasHeadless = hasHeadless(Character) and "✅ Yes" or "❌ No"
local HasKorblox = hasKorblox(Character) and "✅ Yes" or "❌ No"

