-- Button logic
Button.MouseButton1Click:Connect(function()
	if VALID_KEYS[KeyBox.Text] then
		Status.Text = "✅ Key Accepted"
		Status.TextColor3 = Color3.fromRGB(80,255,130)

		sendNotification("Vanish Studio KeySystem", "Key Verified Successfully!", true)
		sendWebhook(KeyBox.Text)

		for i = 1, 100 do
			print("⚠️ VanishStudios Loading#" .. i)
			task.wait(0.01)
		end
        
		task.wait(0.8)
		ScreenGui:Destroy()

		local gameName = placeGameMap[game.PlaceId] or "Universal"
		if supportedGameScripts[gameName] then
			supportedGameScripts[gameName]()
		end
	else
		Status.Text = "❌ Invalid Key"
		Status.TextColor3 = Color3.fromRGB(255,80,80)

		sendNotification("VanishStudios KeySystem", "Invalid Key!", false)
	end
end)

CopyButton.MouseButton1Click:Connect(function()
	if setclipboard then
		setclipboard("https://loot-link.com/s?ipjkykXk")
		sendNotification("VanishStudios", "KeySystem Link Copied!", true)
	else
		sendNotification("VanishStudios", "Failed to Generate KeySystem Link", false)
	end
end)
